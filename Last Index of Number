int lastIndex(int input[], int size, int x) {
  int current,next;
    if(size==0)
    {
        return -1;
    }
    else if(size==1)
    {
        if(input[0]==x)
        {return 0;}
        return -1;
    }
    else
    {
        if (input[size-1]==x) 
        { return size-1;}
        return lastIndex(input,size-1,x);
        return -1;
    } 
}
